    <rule id="6010c9f4-7964-47c6-b711-f96d806f0b4f" name="Strategy Rating &amp; Initiative (Vio)" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="Stratégie &amp; Initiative: Les armées Tau Vior&apos;la ont une Valeur Stratégique de 3. Les formations d&apos;Exo-armures Lourdes, Exo-armures Crisis et Manta disposent d&apos;une valeur d&apos;initiative de 1+, toutes les autres formations ont une initiative de 2+.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="force" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="7f0bed0e-1717-46a1-8225-09866b0dcc87" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Strategie und Initiative: Strategiewert: 3.  • Schwere-Kampfanzüge, Krisisanzüge und Manta -Formationen haben einen Initiativwert von 1+. Andere Formationen haben die Initiativwert 2+.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="17e1c84d-45e8-4447-a3c5-4c3d40b8fd78" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Estrategia e Iniciativa: Factor de Estrategia: 3.  • Valores de Iniciativa: Armaduras de Combate Pesada, Armaduras de Combate Crisis, y Manta: 1+.  • Resto de formaciones: 2+.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="6df4e085-d889-4d87-b048-92b223091d04" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>Vior’la Tau Armies have a strategy rating of 3. Heavy Battlesuit, Crisis Suit, and Manta formations have initiative 1+. All other formations have initiative 2+.</description>
    </rule>
