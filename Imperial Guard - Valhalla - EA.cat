    <rule id="fe793312-0006-4d97-861e-0199e1535bb9" name="Transport (VIW)" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="La capacité de transport: Comme suit-  • Chimera: 2 unités de gardes impériaux.  • Crassus: 7 unités d&apos;Infanterie.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
