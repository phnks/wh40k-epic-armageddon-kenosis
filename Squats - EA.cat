    <rule id="18e797f6-fb24-432c-9ee8-652427dbd28a" name="F-ERC" hidden="false">
      <description>Xenos-Squats-1.04-FERC-2019-1.pdf #03/09/2019 (25/09/2019)</description>
    </rule>
    <rule id="eefb2be3-eeeb-4349-9950-fef73c4dd053" name="Ancient Wisdom" hidden="false">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="force" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="7f0bed0e-1717-46a1-8225-09866b0dcc87" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Sagesse ancestrale: Le joueur Squat peut ignorer la peine -1 à reprendre l&apos;initiative pour une quelconque formation par tour.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Uralteweisheit: Der Squat-Spieler kann die -1-Strafe ignorieren, wenn er versucht, die Initiative für eine beliebige Formation pro Runde beizubehalten.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="17e1c84d-45e8-4447-a3c5-4c3d40b8fd78" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Sabiduría Antigua: El jugador de squat puede ignorar la penalización de -1 para retener la iniciativa para una formación cualquiera por turno.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="6df4e085-d889-4d87-b048-92b223091d04" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>Squat player may ignore the -1 penalty to retain for any one formation per turn.</description>
    </rule>
    <rule id="0d688469-a06a-4777-a519-3fbaa1f29672" name="Automaton (Squat)" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="Automate: Les unités avec cette règle ont une capacité très limitée de mener des actions indépendantes. À moins qu&apos;ils font partie d&apos;une formation qui comprend d&apos;autres unités qui ne possèdent pas cette règle, la formation ne peut prendre les actions suivement: Tenir, Assaut, Regroupement, Tir Soutenu et l&apos;État d&apos;Alerte.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Autómata: Las unidades con esta regla tienen una capacidad muy limitada para llevar a cabo acciones independientes. A menos que formen parte de una formación que incluya otras unidades que no tengan esta regla, la formación sólo puede realizar acciones de Aguantar, Atacar, Mando, Fuego Sostenido y Fuego de Supresión.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="6df4e085-d889-4d87-b048-92b223091d04" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Automat: Einheiten mit dieser Regel haben eine sehr begrenzte Fähigkeit, unabhängige Aktionen durchzuführen. Sofern sie nicht Teil einer Formation sind, die auch andere Einheiten umfasst, für die diese Regel nicht gilt, kann die Formation nur Halten, Angreifen, Marschall, Dauerfeuer und Überwachen ausführen.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="17e1c84d-45e8-4447-a3c5-4c3d40b8fd78" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>Units with this rule have a very limited capacity to carry out independent actions. Unless they are part of a formation that includes other units that do not have this rule the formation can only take Hold, Engage, Marshall, Sustained Fire and Overwatch actions.</description>
    </rule>
    <rule id="c6774b21-2a9f-47ab-a697-9aa32054917c" name="Critical Hit: Overlord" hidden="false">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="force" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="7f0bed0e-1717-46a1-8225-09866b0dcc87" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Touche Critique: L&apos;appareil s&apos;écrase: 3D6cm disperse de façon aléatoire et est détruit. Toute unité à moins de 5 cm du site du crash est soumis à une touche sur le Tableau des Barrages égal à 3PB.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Kritischer Treffer: Overlord: Bewegt 3W6cm in eine zufällige Richtung und wird zerstört. Jede Einheit innerhalb von 5 cm von der Absturzstelle wird auf dem Blastvorlagediagramm einem Treffer von 3 BP ausgesetzt.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="17e1c84d-45e8-4447-a3c5-4c3d40b8fd78" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Daño Crítico: Dirigible Overlord: Dispersa 3D6cm al azar y es destruido. Cualquier unidad dentro de los 5cm del lugar del choque está sujeta a un impacto en la tabla de Plantilla de Artillería igual a 3PA (3BP).">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="6df4e085-d889-4d87-b048-92b223091d04" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>Scatters 3D6cm randomly and is destroyed. Any unit within 5cm of the crash site is subject to a hit on the Blast Template chart equal to 3BP.</description>
    </rule>
    <rule id="4cd6f195-8fe7-415c-b05d-7c985dabc9ad" name="Critical Hit: Colossus" hidden="false">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="force" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="6a7b87a6-e3f4-4ba7-aaac-b20234ded31f" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Touche Critique: Colossus: Prend 1 point de dégâts supplémentaires et se immobilisé. Touches critiques ultérieures détruire l&apos;unité.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Daño Crítico: Colossus: Sufre un punto de daño adicional y queda inmovilizado. Los impactos críticos posteriores destruyen la unidad.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="6df4e085-d889-4d87-b048-92b223091d04" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Kritischer Treffer: Colossus: Nimmt 1 zusätzlichen Schadenspunkt ein und wird immobilisiert. Nachfolgende kritische Treffer zerstören die Einheit.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="17e1c84d-45e8-4447-a3c5-4c3d40b8fd78" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>Takes 1 additional point of damage and is immobilised. Subsequent critical hits destroy the unit.</description>
    </rule>
    <rule id="c8233d22-3a55-4cf7-ab45-df506c637437" name="Critical Hit: Cyclops" hidden="false">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="force" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="6a7b87a6-e3f4-4ba7-aaac-b20234ded31f" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Touche Critique: Cyclope: Prend 1 point de dégâts supplémentaires et ne peut plus tirer son Canon Furie. Touches critiques ultérieures détruire l&apos;unité.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Daño Crítico: Cíclope: Sufre un punto adicional de daño y ya no puede disparar su Cañón Furia. Los impactos críticos subsiguientes destruyen la unidad.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="6df4e085-d889-4d87-b048-92b223091d04" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Kritischer Treffer: Cyclops: Nimmt 1 zusätzlichen Schadenspunkt ein und darf seine Hellfury Canon nicht mehr abfeuern. Nachfolgende kritische Treffer zerstören die Einheit.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="17e1c84d-45e8-4447-a3c5-4c3d40b8fd78" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>Takes 1 additional point of damage and may no longer fire its Hellfury Canon. Subsequent critical hits destroy the unit.</description>
    </rule>
    <rule id="1dc51e0c-aaa8-4538-9c41-5d406b264c5b" name="Critical Hit: Land Train" hidden="false">
