  <sharedRules>
    <rule id="b5a3eab8-1434-49dd-832c-09c0d1bc007d" name="Ally Disposition (MT)" hidden="false">
      <description>Cohesive: Legiones Astartes, Knight Household, Legio Titanicus.
Disruptive: Imperial Militia, Solar Auxilia.</description>
    </rule>
    <rule id="3bb8702e-4f4c-448e-807f-97fe849ab33c" name="Cybernetica Cortex" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="Cortex Cybernétique: Formations contenant des unités avec la règle Cortex Cybernétique subissent une pénalité -1 initiative et ne peuvent pas effectuer Redéploiement ou État d&apos;Alerte actions, s&apos;il n&apos;y a pas des unités amies avec la règle spéciale Contrôleur Cortex dans 15cm de la formation.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Cybernetica-Kortex: Formationen, die Einheiten mit der Cybernetica-Kortex-Regel enthalten, erleiden eine -1-Initiativstrafe und dürfen keine Marsch- oder Overwatch-Aktionen durchführen, wenn es im Umkreis von 15 cm um die Formation keine Einheiten der eigenen Seite mit der Kortex-Controller-Sonderregel gibt.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="17e1c84d-45e8-4447-a3c5-4c3d40b8fd78" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Cortex Cibernético: Las formaciones que contienen unidades con la regla de la Cortex Cibernético sufren una penalización por iniciativa de -1 y no pueden realizar acciones de Marcha o de Fuego de Supresión si no hay unidades amigas con la regla especial del Controlador de Córtex a menos de 15 cm de la formación.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="6df4e085-d889-4d87-b048-92b223091d04" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>Formations containing units with the Cybernetica Cortex rule suffer a -1 initiative penalty and may not perform March or Overwatch actions, if there are no friendly units with the Cortex Controller special rule within 15 cm of the formation.</description>
    </rule>
    <rule id="dc9a07f1-a27b-4fe5-9c63-0bd8f26768c5" name="EpicAU" hidden="false">
      <description>http://epicau.com/mw/index.php/Mechanicum_Taghmata
http://epicau.com/mw/index.php/Mechanicum_Taghmata_Reference_List
http://epicau.com/mw/index.php/Special_Rules
04 july 2020</description>
    </rule>
    <rule id="5da8091e-d1ae-4bf9-b1e8-7572d53e4497" name="Myrmidon" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="Myrmidon: Formations contenant des unités de Destructeurs de Myrmidon ne peut pas prendre une action Redéploiement.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Myrmidon: Formationen, die Einheiten von Myrmidon Destruktoren enthalten, dürfen keine Marschaktion durchführen.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="17e1c84d-45e8-4447-a3c5-4c3d40b8fd78" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Myrmidon: Las formaciones que contienen unidades de Destructores Myrmidon no pueden realizar una acción de Marcha.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="6df4e085-d889-4d87-b048-92b223091d04" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>Formations containing units of Myrmidon Destructors may not take a March action.</description>
    </rule>
    <rule id="e8e6ec52-939b-4f34-a04f-1edf89d0de53" name="Strategy Rating &amp; Initiative (MT)" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="Stratégie &amp; Initiative: Les armées du Mechanicum Taghmata ont une Valeur Stratégique de 3. Toutes les formations ont une Initiative de 2+.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Strategie und Initiative: Strategiewert 3.  • Alle Formationen haben die Initiativwert 2+.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="17e1c84d-45e8-4447-a3c5-4c3d40b8fd78" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Estrategia e Iniciativa: Factor de Estrategia: 3.  • Valores de Iniciativa: Todas las formaciones: 2+.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="6df4e085-d889-4d87-b048-92b223091d04" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>Mechanicum Taghmata armies have a Strategy Rating of 3. All formations have an Initiative rating of 2+.</description>
    </rule>
    <rule id="e87e4a76-8508-4a58-90e8-99f77ab18ac1" name="Transport (MT)" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="La capacité de transport: Comme suit-  • Land Raider: 1 Thallax, Ursarax, Myrmidon, ou 2 des unités suivant: Tech Thrall, Tech Priest, Scyllax.  • Triaros Armoured Conveyor: 2 Thallax, Ursarax, Myrmidon, ou 4 des unités suivant: Tech Thrall, Tech Priest, Scyllax.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>As follows-
Land Raider: 1 Thallax, Ursarax or Myrmidon unit, or 2 of the following units: Tech Thrall, Tech Priest, Scyllax.
Triaros Armoured Conveyor: 2 Thallax, Ursarax or Myrmidon unit, or 4 of the following units: Tech Thrall, Tech Priest, Scyllax.</description>
    </rule>
