    <rule id="e92d8916-9b9f-46a8-bb6c-30191fcd13ee" name="Critical Hit: Holofield Destroyed" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="Dao Crtico: Holopantalla Destruida: La unidad pierde su Holopantalla para el resto del juego. Subsiguientes daos crticos destruirn la unidad.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="6df4e085-d889-4d87-b048-92b223091d04" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Touche Critique: Holo-champ Dtruit: Ne peut plus faire de sauvegarde holographique. La prochaine touche critique dtruira le titan.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Kritischer Treffer: Holofeld Zerstrt: Kann keine Holofeldrettungen mehr aufnehmen. Alle weiteren kritischen Treffer zerstren den Titan.">
