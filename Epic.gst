    </rule>
    <rule id="900561b9-a4f9-4ab3-bb97-2e55ee372cce" name="Oddboyz" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="Eztrambtiko: Sustituye un Kan por un Zper Kan Zzap o un Megakan.">
