    <rule id="031cd053-8897-4ba3-92b0-e4d5e878f1fd" name="Commissars" hidden="false">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="force" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="8183dbe0-128d-44df-9d2f-587c40d576e7" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Commissaires: 0-1 Commissaire peut être ajouté aux Pelotons de Grenadiers, Pelotons de Grenadiers Éclaireurs, Pelotons d’Ingénieurs et Pelotons d’Eclaireurs Death Riders pour 50pts chaque.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="cae033eb-bb6f-4c6c-a304-2f50aaa0236a" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Comisarios: 0-1 Comisario puede ser añadido a las Pelotones de Granaderos, Pelotones de Granaderos Exploradores, Pelotones de Ingenieros y Pelotones de Exploradores Jinetes de la Muerte de Krieg por 50 pts cada uno.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="6df4e085-d889-4d87-b048-92b223091d04" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Kommissare: 0-1 Kommissar kann zu Grenadierzügen, Pfadfinder-Grenadierzügen, Pionierzügen und Todesreiter-Pfadfinderzügen hinzugefügt werden, für jeweils 50 Punkte.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="17e1c84d-45e8-4447-a3c5-4c3d40b8fd78" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>0-1 Commissar may be added to Grenadier Platoons, Scout Grenadier Platoons, Engineer Platoons and Death Riders Scout Platoons for 50pts each.</description>
    </rule>
    <rule id="18e797f6-fb24-432c-9ee8-652427dbd28a" name="F-ERC" hidden="false">
      <description>Compendium Astra Militarum 1.01 - FERC - 2019 #30/08/2019</description>
